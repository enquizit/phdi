  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger",
      "type": "debugpy",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--reload",
        "--port",
        "8080",
        "--log-config",
        "app/log_config.yml"
      ],
      "env": {
        "mpi_db_type": "postgres",
        "mpi_dbname": "testdb",
        "mpi_host": "localhost",
        "mpi_user": "postgres",
        "mpi_password": "postgres",
        "mpi_port": "5432"
      }
    }
  ]
}
