  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger",
      "type": "debugpy",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--reload",
        "--port",
        "8081",
        "--log-config",
        "app/log_config.yml"
      ]
    }
  ]
}
